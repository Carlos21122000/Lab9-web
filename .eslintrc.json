{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb",
        "prettier"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "extends": ["airbnb","pretter"],
        "plugins": ["prettier"],
        "semi":["error","always"],
        "quotes":["error", "doubles"],
        "windows": "\r\n",
       // "allowEmptyCatch": true,
        "no-unused-vars": "warn",
        "prettier/prettier": "error",
        "no-console":"off"
    }
}
